{
    "name": "stickee/cache",
    "description": "PSR-16 Cache Implementation using Zend\\Cache",
    "minimum-stability": "dev",
    "license": "MIT",
    "provides":
    {
        "psr/simplecache-implementation": "~1.0"
    },
    "authors": [
        {
            "name": "Martin Meredith",
            "email": "martin.meredith@stickee.co.uk"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0",
        "psr/simple-cache": "^1.0",
        "zendframework/zend-cache": "^2.7"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "stickee/*": "source",
            "*": "dist"
        }
    },
    "autoload": {
        "psr-4": {
            "Stickee\\Cache\\": "src/"
        }
    }
}
